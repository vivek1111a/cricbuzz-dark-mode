{
  "manifest_version": 3,
  "name": "Cricbuzz Dark Mode and Score Pinning",
  "version": "1.0.1",
  "description": "Enables dark mode and score pinning on Cricbuzz website",
  "key": "",
  "permissions": ["activeTab", "scripting", "storage", "windows"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://www.cricbuzz.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.cricbuzz.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["darkmode.css"],
      "matches": ["*://www.cricbuzz.com/*"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}